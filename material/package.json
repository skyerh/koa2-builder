{
  "name": "example",
  "version": "0.1.0",
  "private": true,
  "apidoc": {
    "template": {
      "forceLanguage": "en"
    },
    "title": "example Server API Document",
    "url": "http://localhost/api"
  },
  "scripts": {
    "start": "NODE_ENV=development node bin/www",
    "dev": "NODE_ENV=development ./node_modules/.bin/nodemon bin/www",
    "prd": "NODE_ENV=production pm2 start bin/www",
    "doc": "NODE_ENV=development apidoc -i ./controllers -o ./public/doc",
    "test": "NODE_ENV=test mocha --timeout 5000 --reporter mochawesome --reporter-options reportDir=./public/report,reportFilename=index"
  },
  "dependencies": {
    "ajv": "^5.5.2",
    "bcrypt": "^1.0.3",
    "crypto-random-string": "^1.0.0",
    "ejs": "^2.6.1",
    "fs-extra": "^5.0.0",
    "jsonwebtoken": "^8.3.0",
    "koa": "^2.5.2",
    "koa-bodyparser": "^3.2.0",
    "koa-helmet": "^3.3.0",
    "koa-json": "^2.0.2",
    "koa-jwt": "^3.3.2",
    "koa-morgan": "^1.0.1",
    "koa-multer": "^1.0.2",
    "koa-onerror": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^3.0.0",
    "koa-views": "^5.2.1",
    "koa2-cors": "^2.0.6",
    "lodash": "^4.17.10",
    "log4js": "^3.0.2",
    "moment": "^2.22.2",
    "mongoose": "^5.2.4",
    "nodemailer": "^4.6.7",
    "pm2": "^2.10.4",
    "redis": "^2.8.0",
    "socket.io": "^2.1.1",
    "superagent": "^3.8.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^4.1.0",
    "mochawesome": "^2.3.1",
    "nodemon": "^1.18.3",
    "supertest": "^3.1.0"
  }
}
