{
  "name": "example",
  "version": "1.0.0",
  "private": true,
  "apidoc": {
    "template": {
      "forceLanguage": "en"
    },
    "title": "example Server API Document",
    "url": "http://localhost/api"
  },
  "scripts": {
    "start": "NODE_ENV=development node bin/www",
    "dev": "NODE_ENV=development ./node_modules/.bin/nodemon bin/www",
    "prd": "NODE_ENV=production pm2 start bin/www",
    "doc": "NODE_ENV=development apidoc -i ./controllers -o ./public/doc",
    "test": "NODE_ENV=test mocha --timeout 5000 --reporter mochawesome --reporter-options reportDir=./public/report,reportFilename=index",
    "tester": "NODE_ENV=development ./node_modules/.bin/nodemon bin/worker.js",
    "worker": "NODE_ENV=production pm2 start bin/worker.js"
  },
  "dependencies": {
    "ajv": "^6.12.3",
    "aws-sdk": "^2.475.0",
    "bcryptjs": "^2.4.3",
    "bee-queue": "^1.2.2",
    "bull-arena": "^2.6.3",
    "crypto-random-string": "^1.0.0",
    "ejs": "^2.6.1",
    "file-type": "^12.0.0",
    "fs-extra": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^3.2.0",
    "koa-helmet": "^3.3.0",
    "koa-json": "^2.0.2",
    "koa-morgan": "^1.0.1",
    "koa-multer": "^1.0.2",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-static": "^3.0.0",
    "koa-views": "^5.2.1",
    "koa2-cors": "^2.0.6",
    "log4js": "^3.0.2",
    "moment": "^2.24.0",
    "mongoose": "^5.5.15",
    "needle": "^2.4.0",
    "nodemailer": "^4.7.0",
    "pm2": "^2.10.4",
    "redis": "^2.8.0",
    "s3-upload-stream": "^1.0.7",
    "sharp": "^0.22.1",
    "socket.io": "^2.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^4.1.0",
    "mochawesome": "^2.3.1",
    "nodemon": "^1.19.1",
    "supertest": "^3.4.2"
  }
}
